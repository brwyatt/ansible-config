[
  {kernel, [
{% if rabbitmq_use_tls_distribution %}
    {ssl_dist_opt, [
      {cacertfile, "{{ rabbitmq_ca_cert_path }}"},
      {certfile,   "{{ rabbitmq_cert_path }}"},
      {keyfile,    "{{ rabbitmq_key_path }}"},
      {verify, verify_peer},
      {fail_if_no_peer_cert, true},
      {server_name_indication, disable}
    ]}
{% endif %}
  ]}
].
